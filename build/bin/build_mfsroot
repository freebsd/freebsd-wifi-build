#!/bin/sh

SCRIPT_NAME="`basename $0`"
SCRIPT_DIR="`dirname $0`"
CUR_DIR="`pwd`"

X_SRCDIR=${CUR_DIR}

# XXX TODO for non-root builds
#
# We need to use the cross-build install program in order to
# take advantage of the mtree-aware upgrades that it's recently
# received.

#
# Unfortunately there's no "easy way" to derive that; so we have
# to derive it ourselves.

INSTALL_PROG="install"

# suck in the per-device options
CFGNAME=$1
shift
. ${SCRIPT_DIR}/../cfg/${CFGNAME} || exit 1

# include the config variable generation code
. ${SCRIPT_DIR}/../lib/cfg.sh || exit 1

# calculate basedir
# XXX this should be generated in cfg.pm!
X_BASEDIR=${SCRIPT_DIR}/../

echo "*** Deleting old file system.."
${INSTALL_PROG} -d ${X_STAGING_FSROOT}
chflags -R noschg ${X_STAGING_FSROOT}
rm -rf ${X_STAGING_FSROOT}
rm -rf ${X_STAGING_TMPDIR}

echo "*** Creating new filesystem..."
mkdir -p ${X_STAGING_FSROOT}
mkdir -p ${X_STAGING_TMPDIR}

# Create needed directories
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/cfg/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/rc.d/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/rc.d/base/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/rc.d/net/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/c/etc/pam.d/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/etc
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/lib/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/bin
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/data/1
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/data/2
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/data/3
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/data/4
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/boot/kernel
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/dev
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/tmp
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/mnt
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/proc
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/var
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/sbin
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/libexec
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/libexec
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/lib/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/bin
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/sbin
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/share/misc/
${INSTALL_PROG} -d ${X_STAGING_FSROOT}/usr/share/tabset/

echo "*** Populating filesystem..."

# MFS tools
${INSTALL_PROG} ${X_DESTDIR}/sbin/mount ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/mount_mfs ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/umount ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/mdconfig ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/fsck_ffs ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/newfs ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/lib/libgeom.so.5 ${X_STAGING_FSROOT}/lib/
${INSTALL_PROG} ${X_DESTDIR}/lib/libufs.so.6 ${X_STAGING_FSROOT}/lib/

# base
${INSTALL_PROG} ${X_DESTDIR}/sbin/init ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/reboot ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/sysctl ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/dmesg ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/kldload ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/kldunload ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/sbin/kldstat ${X_STAGING_FSROOT}/sbin/

# ethernet switch
${INSTALL_PROG} ${X_DESTDIR}/sbin/etherswitchcfg ${X_STAGING_FSROOT}/sbin/

${INSTALL_PROG} ${X_DESTDIR}/sbin/mount_msdosfs ${X_STAGING_FSROOT}/sbin/
${INSTALL_PROG} ${X_DESTDIR}/usr/lib/libkiconv.so ${X_STAGING_FSROOT}/usr/lib/

${INSTALL_PROG} ${X_DESTDIR}/usr/bin/true ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/false ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/sh ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/rm ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/ls ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/ln ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/cp ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/mv ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/df ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/cp ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/cat ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/dd ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/ps ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/sleep ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/hostname ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/mkdir ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/bin/chmod ${X_STAGING_FSROOT}/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/hexdump ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/more ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/nc ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/uptime ${X_STAGING_FSROOT}/usr/bin/
