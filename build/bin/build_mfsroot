${INSTALL_PROG} ${X_DESTDIR}/usr/sbin/gpioctl ${X_STAGING_FSROOT}/usr/sbin/

# editors are useful, honest!
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/ee ${X_STAGING_FSROOT}/usr/bin/

# XXX should symlink gzip to gunzip
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/gzip ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/gunzip ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/libexec/getty ${X_STAGING_FSROOT}/usr/libexec/

# vmstat is useful
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/vmstat ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/systat ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/lib/libdevstat.so.7 ${X_STAGING_FSROOT}/lib/

# libraries are for grep, tar, cpio
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/tar ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/cpio ${X_STAGING_FSROOT}/usr/bin/
${INSTALL_PROG} ${X_DESTDIR}/usr/bin/grep ${X_STAGING_FSROOT}/usr/bin/
